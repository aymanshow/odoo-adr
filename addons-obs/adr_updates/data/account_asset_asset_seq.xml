<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
    
        <record id="seq_type_account_asset" model="ir.sequence.type">
            <field name="name">Asset Number</field>
            <field name="code">asset.number</field>
        </record>
        <record id="seq_account_asset" model="ir.sequence">
            <field name="name">Asset Number</field>
            <field name="code">asset.number</field>
            <field name="prefix"> </field>
            <field name="padding">5</field>
        </record>
      
      <record model="ir.sequence.type" id="seq_type_asset_moved">
            <field name="name">Referencia Movimiento de Activo</field>
            <field name="code">account.asset.move</field>
        </record>

        <record model="ir.sequence" id="seq_account_asset_move">
           <field name="name">Referencia Movimiento de Activo</field>
           <field name="code">account.asset.move</field>
           <field name="prefix">MA/%(year)s/</field>
           <field name="padding">5</field>
           <field name="implementation">no_gap</field>
        </record>
    
    </data>
</openerp>
